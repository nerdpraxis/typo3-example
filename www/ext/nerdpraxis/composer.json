{
	"name": "nerdpraxis/nerdpraxis",
	"description": "nerdpraxis extension",
	"type": "typo3-cms-extension",
	"version": "10.0.0",
	"prefer-stable": true,
	"keywords": ["TYPO3 CMS", "nerdpraxis"],
	"homepage": "https://nerdpraxis.de",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^10.4 ||  dev-master",
		"typo3/cms-backend": "^10.4  || dev-master",
		"typo3/cms-recordlist": "^10.4  || dev-master",
		"typo3/cms-frontend": "^10.4  || dev-master"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"process-timeout": 1000,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "nerdpraxis"
		}
	}
}
